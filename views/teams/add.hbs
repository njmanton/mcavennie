<section>
  <h3>{{ title }}</h3>

  <form action="/teams/add" method="post">
    <div class="row">
      <label class="col-sm-2" for="team">New Team: <span style="color: red;">*</span></label>
      <div class="col">
        <input type="text" name="team" id="proposedTeam" />
        <span role="alert" class="intrainput" id="team-dup"></span>
      </div>
    </div>
    <div class="row">
      <label class="col-sm-2" for="teamCountry">Country:</label>
      <div class="col">
        <input type="text" id="teamCountry" />
      </div>
    </div>
    <div class="row">
      <input type="hidden" name="country" id="countryCode" />
      <input type="submit" class="btn btn-info" id="addTeam" />
    </div>
  </form>

</section>
<style>
  .easy-autocomplete input {
    border-width: 2px;
    border-radius: 0;
    border-style: inset;
    border-color: initial;
    padding: inherit;
    box-shadow: none;
  }
</style>